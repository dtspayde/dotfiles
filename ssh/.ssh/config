Host login.jlab.org
    User spayde
    ForwardX11 yes
    ForwardX11Trusted yes
    SetEnv LANG=C
    GSSAPIAuthentication no

Host ifarm* ifarm*.jlab.org ftp.jlab.org
    User spayde
    ForwardX11 yes
    ForwardX11Trusted yes
    SetEnv LANG=C
    GSSAPIAuthentication no
    ProxyJump login.jlab.org

Host nuclear.hendrix.edu
    User spayde
    Hostname 96.31.222.147

Host *.nuclear.local
    User spayde
    # LocalForward 9090 localhost:9090
    ProxyJump nuclear.hendrix.edu
